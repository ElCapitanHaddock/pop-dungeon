public class BoneHurtingJuice
{
    String variable = "";
    public BoneHurtingJuice()
    {
        variable = "a variable";
    }
    public void a_mutator_method()
    {
        variable = "a mutated variable";
    }
    public String an_accessor_method()
    {
        return variable;
    }
}

