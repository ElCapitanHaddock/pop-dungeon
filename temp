    public static void podrace()
    {
        for (int i = 0; i < 7; i++) {
            System.out.println("I'll try looping! That's a good trick!");
        }
    }
    public static String execute_order(int order)
    {
        if (order == 66)
        {
            return "kill the men";
        }
        else if (order == 66)
        {
            return "kill the women and the younglings too";
        }
        else {
            return "it's treason then";
        }
    }
    public static int kill_jedi(int survivors) {
        if (survivors <= 3) {
            return 2;
        }
        return kill_jedi(survivors - 1);
    }
